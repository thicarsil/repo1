{
    "dataSource": "milestones",
    "prefix": "",
    "groupBy": {
        "Melhorias:": ["feature", "internal"],
        "Correções de Bugs:": ["bug"],
        "Testes:": ["teste"]
    },
    "milestoneMatch": "(Lançamento da Versão {{tag_name}})",
    "template": {
        "issue": "- [{{text}}]({{url}}) {{name}}",
        "changelogTitle": "# Lançamento da versão {{release}}\n\n",
        "release": "## {{release}} ({{date}})\n{{body}}"
    }
}

